{
  "name": "kamox-monorepo",
  "version": "0.1.2",
  "private": true,
  "description": "Web Extension Dev Server",
  "workspaces": [
    "packages/*",
    "cli"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm test --workspaces",
    "clean": "rm -rf dist node_modules packages/*/dist packages/*/node_modules cli/dist cli/node_modules examples/*/dist examples/*/node_modules",
    "cli": "npm run start --workspace=kamox-cli"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "electron": "^28.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "author": "KamoX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwabuchi404/kamox.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
